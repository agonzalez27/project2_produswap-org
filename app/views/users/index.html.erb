<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li><%= user.first_name %> <%= user.last_name %> <%= user.email %>
  <% end %>
</ul>
<% if current_user %>
  Logged in as: <%= current_user.first_name %> <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= link_to 'Log In', login_path %>
  <%= link_to 'Sign Up', new_user_path %>
<% end %>
